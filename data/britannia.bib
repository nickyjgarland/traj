@article{ ISI:000369897600006,
Author = {Bailliot, Magali},
Title = {{Roman Magic Figurines from the Western Provinces of the Roman Empire: An
   Archaeological Survey}},
Journal = {{BRITANNIA}},
Year = {{2015}},
Volume = {{46}},
Pages = {{93-110}},
Abstract = {{This paper deals with magic figurines from the Western provinces of the
   Roman Empire based on an inventory of twelve figurines and their
   archaeological context. It underlines the place of the figurines in the
   ritual of defixio and demonstrates that complex curse rituals such as
   those described in the Greek Magical Papyri (GMP) were not performed
   only in the Mediterranean basin. It also notes that these magic Western
   figurines are often found in important places (such as cities and large
