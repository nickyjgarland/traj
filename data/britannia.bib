@article{ ISI:000492053000011,
Author = {Keppie, Lawrence},
Title = {{The Roman Fortress at Carpow, Perthshire: An Alternative Interpretation
   of the Gates and their Dedicatory Inscriptions}},
Journal = {{BRITANNIA}},
Year = {{2019}},
Volume = {{50}},
Pages = {{265-283}},
Month = {{NOV}},
Abstract = {{The Roman fortress at Carpow, Perthshire, was excavated in 1961-62 and
   1964-79. The gates of the fortress are the key to our understanding of
   its occupational history. The suggestion made here is that they were
   initially constructed in timber, at the same time as the associated turf
   rampart, but were soon replaced in stone specifically to support heavy
   dedication slabs set above them. A new arrangement of the surviving
   inscribed fragments at the East Gate is proposed, and new readings
   adopted, which confirm their traditional Severan dating.}},
