@article{ ISI:000320431700001,
Author = {Logan, William},
Title = {{Australia, Indigenous peoples and World Heritage from Kakadu to Cape
   York: State Party behaviour under the World Heritage Convention}},
Journal = {{JOURNAL OF SOCIAL ARCHAEOLOGY}},
