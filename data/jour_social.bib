@article{ ISI:000259681200004,
Author = {Insoll, Timothy},
Title = {{Negotiating the archaeology of destiny - An exploration of interpretive
   possibilities through Tallensi Shrines}},
Journal = {{JOURNAL OF SOCIAL ARCHAEOLOGY}},
Year = {{2008}},
Volume = {{8}},
Number = {{3}},
Pages = {{380-403}},
Month = {{OCT}},
Abstract = {{The concept of `destiny' is perhaps irrelevant to much of modern
   thinking in Western Europe and North America and hence fails to enter
   archaeological vocabulary. This is potentially an omission of
   consequence, for social psychology indicates that the belief that life
   is beyond the control of human agency can be a powerful one. Drawing
   upon ethnographic and archaeological data from the Tallensi of Northern
   Ghana, the potential resonance of `destiny' in archaeological
   interpretation is explored with reference to some examples of deposition
   from later British prehistory.}},
