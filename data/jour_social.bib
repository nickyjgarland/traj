@article{ ISI:000274805000006,
Author = {Lucero, Lisa J.},
Title = {{Materialized cosmology among ancient Maya commoners}},
Journal = {{JOURNAL OF SOCIAL ARCHAEOLOGY}},
Year = {{2010}},
Volume = {{10}},
Number = {{1}},
Pages = {{138-167}},
Month = {{FEB}},
Abstract = {{Classic Maya inscriptions and iconography reveal more than just royal
   customs since their origins lie in traditional Maya practices. They
   provide a key to unlocking how commoners created their own domestic
   universe. To explore how commoners mapped their cosmology and recorded
   their history, I discuss domestic ceramic clusters based on color,
   placement, and association with other artifacts at the minor center of
   Saturday Creek, Belize. Results show that cached items served to
   contextualize their place in the cosmos. Commoners may not have had the
   written word, but they had the means to record their own history, one
   with which they interacted daily - under their feet, within walls, and
   under their roof.}},
